{
  "name": "graphql-file-sender",
  "version": "1.0.23",
  "description": "custom Node Express, Apollo Client plugin to send pdfs over graphql",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenSimmers/graphQL-file-sender.git"
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "typescript",
    "pdf",
    "file",
    "download"
  ],
  "author": "Ben Simmers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenSimmers/graphQL-file-sender/issues"
  },
  "homepage": "https://github.com/BenSimmers/graphQL-file-sender#readme",
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.4.1",
    "@types/path-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "fs": "^0.0.1-security",
    "funfix": "^7.0.1",
    "graphql": "^15.8.0",
    "immutable": "^4.3.0",
    "mime-types": "^2.1.35"
  },
  "files": [
    "lib/**/*"
  ]
}
