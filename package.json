  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/react": "^18.2.14",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "funfix": "^7.0.1",
